start on starting <%= app %>-<%= process.name %>
stop on stopping <%= app %>-<%= process.name %>
respawn

setuid <%= user %>
setgid <%= user %>

script
  cd <%= engine.directory %>
  export PATH=$PATH:<%= engine.directory %>/bin
  . .opdemand
  exec <%= process.command %> >> <%= log_root %>/<%=process.name%>-<%=num%>.log 2>&1
end script