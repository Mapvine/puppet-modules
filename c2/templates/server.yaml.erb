C2_OPERATORS: <%= @c2_operators %>

# url settings
COOKIE_DOMAIN: <%= @cookie_domain %>
C2SERVER_URL: http://<%= @cookie_domain %>:<%= @port %>
C2PROXY_URL: http://<%= @cookie_domain %>:<%= @port %>

# smtp settings
ENABLE_SMTP: <%= @enable_smtp %>
SMTP_PASSWORD: <%= @smtp_password %>

# zendesk settings
ZENDESK_AUTH_URL: <%= @zendesk_auth_url %>
ZENDESK_AUTH_TOKEN: <%= @zendesk_auth_token %>

# oauth config for github/c2 development
OAUTH_CLIENT_ID: <%= @github_oauth_id %>
OAUTH_CLIENT_SECRET: <%= @github_oauth_secret %>

# chargify settings
CHARGIFY_URL: <%= @chargify_url %>
CHARGIFY_APIKEY: <%= @chargify_apikey %>
